<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:interpolator="@android:interpolator/fast_out_slow_in"
    android:transitionOrdering="together">

    <changeBounds android:duration="@android:integer/config_longAnimTime">

        <pathMotion
            class="com.example.prashant.materialdesign.ui.transitions.GravityArcMotion"
            android:maximumAngle="90" />

    </changeBounds>

    <transition
        class="com.example.prashant.materialdesign.ui.transitions.LiftOff"
        android:duration="@android:integer/config_longAnimTime"
        app:initialElevation="8dp"/>

</transitionSet>
